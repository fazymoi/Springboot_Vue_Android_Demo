<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androiddemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!--写-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <!--读-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:requestLegacyExternalStorage="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"

        android:theme="@style/Theme.AndroidDemo">
        <activity
            android:name=".MainActivity"
            android:label="美食推荐系统">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="密码登录" />
        <activity android:name=".CommentManage"
            android:label="我的评论">

        </activity>
        <activity android:name=".Foodpage"
            android:label="美食主页">

        </activity>
        <activity android:name=".FoodComment"
            android:label="美食评论">

        </activity>
        <activity android:name=".UpdateComment"
            android:label="修改评论">

        </activity>
        <activity android:name=".PersonalInfo"
            android:label="基本信息">
        </activity>
        <activity
            android:name=".StorepageActivity"
            android:label="商家主页" />
        <activity
            android:name=".KeysActivity"
            android:label="修改密码" />
        <activity
            android:name=".SearchActivity"
            android:label="搜索" />
        <activity
            android:name=".PersonalActivity"
            android:label="个人中心" />
        <activity
            android:name=".RegisterActivity"
            android:label="用户注册" />
        <activity
            android:name=".CollectionManage"
            android:label="我的收藏" />
        <activity
            android:name=".MystoreActivity"
            android:label="我的商家" />

    </application>

</manifest>